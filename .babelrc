{
	"presets": ["es2017"],
	"plugins": [
		"transform-es2015-modules-commonjs",
		"transform-object-rest-spread",
		"babel-root-import"
	],
	"env": {
		"production": {
			"plugins": [
				"transform-es2015-modules-commonjs",
				"transform-object-rest-spread",
				[ "babel-root-import", { "rootPathSuffix": "src"} ]
			]
		}
	}
}
