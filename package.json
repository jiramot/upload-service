{
  "name": "upload",
  "version": "1.0.0",
  "description": "File upload service",
  "main": "app.js",
  "scripts": {
    "build": "BABEL_ENV=production babel src -d dist --copy-files --ignore *.test.js",
    "start": "cd dist && NODE_ENV=development node app.js",
    "prod": "cd dist && NODE_ENV=production node app.js",
    "staging": "cd dist && NODE_ENV=staging node app.js",
    "docker": "cd dist && NODE_ENV=docker node app.js",
    "test": "cd src && JWT_SECRET='server secret' NODE_ENV=test mocha --compilers js:babel-register test --recursive --reporter mocha-jenkins-reporter",
    "test:watch": "nodemon --exec yarn test",
    "coverage": "cd src && JWT_SECRET='server secret' NODE_ENV=test babel-node ../node_modules/.bin/babel-istanbul cover --report cobertura _mocha -- test --recursive"
  },
  "keywords": [
    "upload",
    "micro-service",
    "nodejs"
  ],
  "author": "Watchanon Numnam <jiramot@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "api-health-check": "^0.1.5",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "child-process-promise": "^2.2.0",
    "connect-timeout": "^1.8.0",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "fs-promise": "^1.0.0",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "multer": "^1.1.0",
    "pify": "^2.3.0",
    "sinon": "^1.17.4",
    "underscore": "^1.8.3",
    "uuid": "^3.0.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-istanbul": "^0.11.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-root-import": "^4.1.3",
    "chai": "^3.5.0",
    "form-data": "^2.1.2",
    "fs-temp": "^1.1.0",
    "gm": "^1.23.0",
    "istanbul": "^0.4.3",
    "mocha": "^3.2.0",
    "mocha-jenkins-reporter": "^0.3.1",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.9",
    "rimraf": "^2.5.2",
    "supertest": "^2.0.1",
    "yarn": "^0.17.9"
  }
}
