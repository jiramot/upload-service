{
  "name": "upload",
  "version": "1.0.0",
  "description": "File upload service",
  "main": "app.js",
  "scripts": {
    "build": "BABEL_ENV=production babel src -d dist --copy-files --ignore *.test.js",
    "start": "cd dist && NODE_ENV=development node app.js",
    "prod": "cd dist && NODE_ENV=production node app.js",
    "staging": "cd dist && NODE_ENV=staging node app.js",
    "docker": "cd dist && NODE_ENV=docker node app.js",
    "test": "cd src && JWT_SECRET='server secret' NODE_ENV=test mocha --compilers js:babel-register test --recursive --reporter mocha-jenkins-reporter",
    "test:watch": "nodemon --exec yarn test",
    "coverage": "cd src && JWT_SECRET='server secret' NODE_ENV=test babel-node ../node_modules/.bin/babel-istanbul cover --report cobertura _mocha -- test --recursive"
  },
  "keywords": [
    "upload",
    "micro-service",
    "nodejs"
  ],
  "author": "Watchanon Numnam <jiramot@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "api-health-check": ">=0.1.5",
    "bluebird": ">=3.7.2",
    "body-parser": ">=1.19.0",
    "child-process-promise": ">=2.2.1",
    "connect-timeout": ">=1.9.0",
    "cors": ">=2.8.5",
    "express": ">=4.17.1",
    "fs-promise": "^2.0.3",
    "lodash": ">=4.17.20",
    "mkdirp": ">=1.0.4",
    "moment": ">=2.29.1",
    "multer": ">=1.4.2",
    "pify": ">=5.0.0",
    "sinon": ">=9.2.1",
    "underscore": ">=1.11.0",
    "uuid": ">=8.3.1",
    "winston": ">=3.3.3"
  },
  "devDependencies": {
    "babel-cli": ">=6.26.0",
    "braces": ">=2.3.1",
    "babel-istanbul": ">=0.12.2",
    "babel-plugin-transform-es2015-modules-commonjs": ">=6.26.2",
    "babel-plugin-transform-object-rest-spread": ">=6.26.0",
    "babel-preset-es2017": ">=6.16.0",
    "babel-register": ">=6.26.0",
    "babel-root-import": ">=4.1.8",
    "chai": ">=4.2.0",
    "form-data": ">=3.0.0",
    "fs-temp": ">=1.2.1",
    "gm": ">=1.23.1",
    "istanbul": ">=0.4.3",
    "mocha": ">=8.2.1",
    "mocha-jenkins-reporter": ">=0.4.5",
    "nodemon": ">=2.0.6",
    "proxyquire": ">=2.1.3",
    "rimraf": ">=3.0.2",
    "supertest": ">=6.0.0",
    "yarn": ">=1.22.10"
  }
}
